<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Text layoutX="14.0" layoutY="40.0" text="Analyse de l'Usure et Valeur des Matériels" wrappingWidth="572.0">
            <font>
                <Font name="Montserrat SemiBold" size="20.0" />
            </font>
        </Text>
        <BarChart fx:id="materielBarChart" layoutX="14.0" layoutY="60.0" prefHeight="330.0" prefWidth="572.0" title="Statistiques des Matériels">
            <xAxis>
                <CategoryAxis label="Type de Matériel" />
            </xAxis>
            <yAxis>
                <NumberAxis label="Valeur Résiduelle (TND)" />
            </yAxis>
        </BarChart>
    </children>
</AnchorPane>